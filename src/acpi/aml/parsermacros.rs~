#[macro_export]
macro_rules! parser_selector {
    {$data:expr, $func:expr} => {
        match $func($data) {
            Ok(res) => return Ok(res),
            Err(AmlInternalError::AmlInvalidOpCode) => (),
            Err(e) => return Err(e)
        }
    };
    {$data:expr, $func:expr, $($funcs:expr),+} => {
        parser_selector! {$data, $func};
        parser_selector! {$data, $($funcs),*};
    };
}
